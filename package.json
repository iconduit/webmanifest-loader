{
  "name": "@iconduit/webmanifest-loader",
  "version": "0.0.0",
  "description": "A Webpack loader for .webmanifest files",
  "keywords": [
    "app",
    "iconduit",
    "import",
    "loader",
    "manifest",
    "progressive",
    "pwa",
    "web",
    "webmanifest",
    "webpack"
  ],
  "repository": "iconduit/webmanifest-loader",
  "bugs": "https://github.com/iconduit/webmanifest-loader/issues",
  "homepage": "https://github.com/iconduit/webmanifest-loader",
  "author": "Erin Millard <contact@erin.id.au>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "artifacts/dist/esm/index.js",
  "types": "artifacts/dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./artifacts/dist/types/index.d.ts",
      "import": "./artifacts/dist/esm/index.js",
      "require": "./artifacts/dist/cjs/index.js",
      "default": "./artifacts/dist/esm/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "/artifacts/dist/"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "prepublishOnly": "make artifacts/dist"
  },
  "dependencies": {
    "loader-utils": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/eslint__js": "^8.42.3",
    "@types/loader-utils": "^2.0.6",
    "@types/node": "^22.8.5",
    "@vitest/coverage-v8": "^2.1.4",
    "@vitest/eslint-plugin": "^1.1.7",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.12.0",
    "eslint-plugin-promise": "^7.1.0",
    "globals": "^15.11.0",
    "html-bundler-webpack-plugin": "^4.1.4",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.12.2",
    "vitest": "^2.1.4",
    "webpack": "^5 <5.96.0",
    "webpack-cli": "^5.1.4"
  }
}
